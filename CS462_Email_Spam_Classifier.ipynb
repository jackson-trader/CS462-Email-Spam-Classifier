      "source": [
        "\n",
        "# 1) Labels (ham=0, spam=1)\n",
        "y_train = (dfTraining[\"Category\"] == \"spam\").astype(int).to_numpy()\n",
        "y_test  = (dfTest[\"Category\"] == \"spam\").astype(int).to_numpy()\n",
        "\n",
        "# 2) Text -> features (fit ONLY on training to avoid leakage)\n",
        "vectorizer = TfidfVectorizer(\n",
        "    lowercase=False,      # you already lowercased earlier\n",
        "    stop_words=\"english\",\n",
        "    ngram_range=(1, 2),   # unigrams + bigrams\n",
        "    min_df=2\n",
        ")\n",
        "\n",
        "X_train = vectorizer.fit_transform(dfTraining[\"Message\"])\n",
        "X_test  = vectorizer.transform(dfTest[\"Message\"])\n",
        "\n",
        "print(\"X_train shape:\", X_train)\n",
        "print(\"X_test shape:\", X_test)"
      ]
